-- Crear base de datos.--

CREATE database Grupo;

-- Crear tabla Alumnos --
use Grupo
CREATE TABLE Alumnos 
(
  idMatricula INT NOT NULL,
  Nombre VARCHAR(45) NOT NULL,
  Apellido VARCHAR(45) NOT NULL,
  Edad INT NOT NULL,
  Carrera VARCHAR(45) NOT NULL,
  PRIMARY KEY (idMatricula)
  );
	  
